{
  "presets": [
    "next/babel",
    "react",
    [
      "latest",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./app"
        ],
        "alias": {
          "app": "./",
          "components": "./components",
          "modules": "./modules",
          "utils": "./utils"
        }
      }
    ],
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
